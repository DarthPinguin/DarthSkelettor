    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/router": "6.0.7",
    "@ngx-loading-bar/http-client": "1.0.1",
    "@ngx-loading-bar/router": "1.0.1",
    "@ngx-translate/core": "9.0.1",
