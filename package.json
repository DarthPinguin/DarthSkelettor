    "core-js": "2.5.7",
    "font-awesome": "4.7.0",
    "gettext-parser": "2.0.0",
    "jquery": "3.3.1",
    "lodash": "4.17.10",
    "ngx-cookie-service": "1.0.10",
    "ngx-translate-extract": "1.0.0",
    "rxjs": "6.2.1",
    "url-search-params-polyfill": "4.0.0",
    "webpack": "4.15.0",
    "zone.js": "0.8.26",
