{
  "name": "osiris.organization.ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node ./banner.js & ng lint & ng serve --port=4200 --base-href=/DarthSkelettor/",
    "build": "node ./banner.js & ng build --prod --aot --source-map=false --base-href /DarthSkelettor/",
    "build-int": "node ./banner.js & ng build --aot --configuration=int --source-map=false --base-href /DarthSkelettor/ --output-hashing=none",
    "build-sta": "node ./banner.js & ng build --aot --configuration=sta --source-map=false --base-href /DarthSkelettor/ --output-hashing=none",
    "build-prod": "ng build --aot --prod --configuration=production --source-map=false --base-href /DarthSkelettor/ --output-hashing=none",
    "postinstall": "node ./patchV6ForPO.js",
    "extract-labels": "ngx-translate-extract -i ./src -o ./src/assets/i18n/fr.po ./src/assets/i18n/nl.po -f pot -m _",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.7",
