    "lodash": "4.17.4",
    "ngx-cookie-service": "1.0.9",
    "ngx-translate-extract": "1.0.0",
    "rxjs": "6.2.0",
    "rxjs-compat": "^6.0.0-rc.0",
    "url-search-params-polyfill": "3.0.0",
